{
    "class": "CommandLineTool",
    "name": "colabdesign-dev",
    "description": "one-pot design of protein backbones and sequences",
    "author": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
        "mv /inputs/*.pdb /inputs/target_protein.pdb && mv /inputs/*.yaml /inputs/config.yaml;",
        "echo 'Attempting main.py...';",
        "python main.py;",
        "echo 'main.py complete!';"
      ],
    "dockerPull": "quay.io/labdao/colabdesign:pr-745@sha256:c43df6bf0851e6d4cf8fed80ea1ab4c16556718c5b3fa5bbae42462950c4ef44",
    "gpuBool": true,
    "networkBool": true,
    "inputs": {
      "protein": {
        "type": "File",
        "item": "",
        "glob": ["*.pdb"]
      },
      "config": {
        "type": "File",
        "item": "",
        "glob": ["*.yaml"]
      }
    },
    "outputs": {
      "compressed_designs": {
        "type": "Array",
        "item": "File",
        "glob": ["*.zip"]
      }
    }
  }
