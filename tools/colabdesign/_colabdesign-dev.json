{
    "class": "CommandLineTool",
    "name": "colabdesign-dev",
    "description": "one-pot design of protein backbones and sequences",
    "author": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
        "mv /inputs/*.pdb /inputs/target_protein.pdb && mv /inputs/*.yaml /inputs/config.yaml;",
        "echo 'Attempting main.py...';",
        "python -u main.py;",
        "echo 'main.py complete!';"
      ],
    "dockerPull": "quay.io/labdao/colabdesign:6cc5ae4@sha256:9761286d6bce72bb6786e73e9da72a18b328fee70fd49d226a0999aef627e654",
    "gpuBool": true,
    "networkBool": true,
    "cpu": 3,
    "memoryGB": 12,
    "inputs": {
      "protein": {
        "type": "File",
        "item": "",
        "glob": ["*.pdb"]
      },
      "config": {
        "type": "File",
        "item": "",
        "glob": ["*.yaml"]
      }
    },
    "outputs": {
      "compressed_designs": {
        "type": "Array",
        "item": "File",
        "glob": ["*.zip"]
      }
    }
  }
