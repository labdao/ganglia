{
  "name": "colabdesign gateway v0.4",
  "description": "RFdiffusion is a method for structure generation, with or without conditional information (a motif, target etc). It can perform a whole range of protein design challenges.",
  "author": "sokrypton",
  "github": "https://github.com/sokrypton/ColabDesign",
  "paper": "https://www.nature.com/articles/s41586-023-06415-8",
  "task": "protein design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "docker.io/openzyme/colabdesign-gateway@sha256:0b452233b0fa39e7a220c3f1be9f31010d175595207b91c62349d690aff38626",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "p_contact_domain_masking": {
      "type": "number",
      "description": "Probability of masking a contact domain",
      "array": false,
      "glob": [
        ""
      ],
      "default": ".6",
      "min": "0",
      "max": "1",
      "example": ".6",
      "grouping": "auto prompt parameters",
      "position": "103",
      "required": true
    },
    "p_noncontact_domain_masking": {
      "type": "number",
      "description": "Probability of masking a non-contact domain",
      "array": false,
      "glob": [
        ""
      ],
      "default": ".1",
      "min": "0",
      "max": "1",
      "example": ".1",
      "grouping": "auto prompt parameters",
      "position": "104",
      "required": true
    },
    "cutoff": {
      "type": "number",
      "description": "Distance cutoff to define inter-protein contact residues [Angstrom]",
      "array": false,
      "glob": [
        ""
      ],
      "default": "5.0",
      "min": "0",
      "max": "20",
      "example": "5.0",
      "grouping": "auto prompt parameters",
      "position": "105",
      "required": true
    },
    "n_samples": {
      "type": "number",
      "description": "Number of auto prompts generated",
      "array": false,
      "glob": [
        ""
      ],
      "default": "16",
      "min": "1",
      "max": "64",
      "example": "16",
      "grouping": "auto prompt parameters",
      "position": "106",
      "required": true
    },
    "domain_distance_threshold": {
      "type": "number",
      "description": "Minimum number of residues separating distinct domains",
      "array": false,
      "glob": [
        ""
      ],
      "default": "6",
      "min": "0",
      "max": "20",
      "example": "6",
      "grouping": "auto prompt parameters",
      "position": "107",
      "required": true
    },
    "number_of_binders": {
      "type": "number",
      "description": "The number of protein binders to be designed",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "1",
      "max": "32",
      "example": "2",
      "grouping": "binder",
      "position": "202",
      "required": true
    },
    "target_chain": {
      "type": "string",
      "description": "The chain of the target protein to design against.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "A",
      "min": "",
      "max": "",
      "example": "A",
      "grouping": "target",
      "position": "102",
      "required": true
    },
    "target_protein": {
      "type": "file",
      "description": "The target protein to design against.",
      "array": false,
      "glob": [
        "*.pdb"
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "my_target_protein.pdb",
      "grouping": "target",
      "position": "101",
      "required": true
    }
  },
  "outputs": {
    "all_outputs": {
      "type": "Array",
      "item": "File",
      "glob": [
        "*.zip"
      ]
    },
    "best_design": {
      "type": "File",
      "item": "",
      "glob": [
        "*_best.pdb"
      ]
    },
    "scores": {
      "type": "File",
      "item": "",
      "glob": [
        "*_scores.csv"
      ]
    },
    "collected_scores": {
      "type": "File",
      "item": "",
      "glob": [
        "*_collected_scores.csv"
      ]
    }
  }
}
