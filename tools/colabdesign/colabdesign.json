{
    "class": "CommandLineTool",
    "name": "colabdesign",
    "description": "one-pot design of protein backbones and sequences",
    "author": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
        "mv /inputs/*.pdb /inputs/target_protein.pdb;",
        "python main.py;",
        "mv *.zip /outputs;"
      ],
    "dockerPull": "docker.io/niklastr/colabdesign@sha256:e52e0cd4829b768f4f885453e6e07f508c44c4265e1659b898d5622a52c97250",
    "gpuBool": true,
    "networkBool": true,
    "inputs": {
      "protein": {
        "type": "File",
        "item": "",
        "glob": ["*.pdb"]
      },
      "config": {
        "type": "File",
        "item": "",
        "glob": ["*.yaml"]
      }
    },
    "outputs": {
      "compressed_results": {
        "type": "File",
        "item": "",
        "glob": ["*.zip"]
      }
    }
  }
  