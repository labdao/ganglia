{
  "class": "CommandLineTool",
  "name": "colabdesign-dev",
  "description": "one-pot design of protein backbones and sequences",
  "author": "",
  "baseCommand": ["/bin/bash", "-c"],
  "arguments": [
      "mv /inputs/*.pdb /inputs/target_protein.pdb && mv /inputs/*.yaml /inputs/config.yaml;",
      "python main.py;",
      "mv *.zip /outputs;"
    ],
  "dockerPull": "docker.io/niklastr/colabdesign@sha256:536e94a52110eed668416da465178f6a3f97c6a69072b259f00bcd0bde41103b",
  "gpuBool": true,
  "networkBool": true,
  "inputs": {
    "protein": {
      "type": "File",
      "item": "",
      "glob": ["*.pdb"]
    },
    "config": {
      "type": "File",
      "item": "",
      "glob": ["*.yaml"]
    }
  },
  "outputs": {
    "compressed_results": {
      "type": "Array",
      "item": "File",
      "glob": ["*.zip"]
    }
  }
}