{
    "name": "design_with_colabdesign_v7",
    "description": "Design with colabdesign",
    "task": "protein design",
    "taskCategory": "protein-binder-design",
    "toolType": "ray",
    "rayServiceEndpoint": "/design_with_colabdesign",
    "checkpointCompatible": true,
    "xAxis": "plddt",
    "yAxis": "i_pae",
    "computeCost": 10,
    "inputs": {
      "pdb": {
        "type": "file",
        "description": "The target protein to design against.",
        "array": false,
        "glob": [
          "*.pdb"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "my_target_protein.pdb",
        "grouping": "target",
        "position": "101",
        "required": true
      },
      "target_chain": {
        "type": "string",
        "description": "The chain of the target protein to design against.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "A",
        "min": "",
        "max": "",
        "example": "A",
        "grouping": "target",
        "position": "102",
        "required": true
      },
      "binder_chain": {
        "type": "string",
        "description": "The chain of the target protein to design against.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "B",
        "min": "",
        "max": "",
        "example": "B",
        "grouping": "target",
        "position": "103",
        "required": true
      },
      "binder_length": {
        "type": "number",
        "description": "The length of the protein binder that is being designed",
        "array": false,
        "glob": [
          ""
        ],
        "default": "85",
        "min": "1",
        "max": "1000",
        "example": "85",
        "grouping": "_advanced",
        "position": "201",
        "required": false
      },
      "contig": {
        "type": "string",
        "default": "",
        "description": "The complete prompt or 'contig' passed to the diffusion model. This overrides other GUI inputs.",
        "example": "A1-283:B1-6/1/B8-20/22/B43-47",
        "grouping": "_advanced",
        "position": "202",
        "required": false
      },
      "hotspots": {
        "type": "string",
        "description": "The residues that should be in contact with the binder. e.g. A30,A33,A34",
        "array": false,
        "glob": [
          ""
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "A30,A33,A34",
        "grouping": "_advanced",
        "position": "203",
        "required": false
      },
      "start_residue": {
        "type": "number",
        "description": "The start residue of the target protein.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "5",
        "min": "1",
        "max": "",
        "example": "5",
        "grouping": "_advanced",
        "position": "204",
        "required": false
      },
      "end_residue": {
        "type": "number",
        "description": "The end residue of the target protein.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "107",
        "min": "1",
        "max": "",
        "example": "107",
        "grouping": "_advanced",
        "position": "205",
        "required": false
      }
    },
    "outputs": {
      "string_message": {
        "type": "File",
        "glob": ["*.json"]
      }
    }
}