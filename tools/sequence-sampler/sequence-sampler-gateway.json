{
    "name": "sequence-sampler v0.2 (beta)",
    "description": "Evolutionary sampling of sequences.",
    "author": "Convexity Labs",
    "github": "...",
    "paper": "...",
    "task": "sequence design",
    "baseCommand": [
    ],
    "arguments": [
    ],
    "dockerPull": "quay.io/labdao/...",
    "gpuBool": true,
    "memoryGB": 12,
    "cpu": 3,
    "networkBool": true,
    "inputs": {
      "target_template_complex": {
        "type": "file",
        "description": "pdb file containing the complex of target and binder template",
        "array": false,
        "glob": [
          ".pdb"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "binder_template_sequence.fasta",
        "grouping": "protein complex",
        "position": "101",
        "required": true
      },
      "target_chain": {
        "type": "string",
        "description": "target chain label",
        "array": false,
        "glob": [
          ""
        ],
        "default": "A",
        "min": "",
        "max": "",
        "example": "A",
        "grouping": "protein complex",
        "position": "102",
        "required": true
      },
      "binder_chain": {
        "type": "string",
        "description": "binder chain label",
        "array": false,
        "glob": [
          ""
        ],
        "default": "B",
        "min": "",
        "max": "",
        "example": "B",
        "grouping": "protein complex",
        "position": "103",
        "required": true
      },
      "target_seq": {
        "type": "file",
        "description": "Sequence of target protein.",
        "array": false,
        "glob": [
          ".fasta"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "target_sequence.fasta",
        "grouping": "protein complex",
        "position": "102",
        "required": true
      },
      "binder_template_sequence": {
        "type": "file",
        "description": "Binder template from which the sequence sampling is initialised",
        "array": false,
        "glob": [
          ".fasta"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "binder_template_sequence.fasta",
        "grouping": "protein complex",
        "position": "103",
        "required": true
      },
      "number_of_evo_cycles": {
        "type": "number",
        "description": "Number of evolutionary cycles",
        "array": false,
        "glob": [
          ""
        ],
        "default": "15",
        "min": "1",
        "max": "60",
        "example": "10",
        "grouping": "evolution parameters",
        "position": "201",
        "required": true
      },
      "max_levenshtein_step_size": {
        "type": "number",
        "description": "Maximum Levenshtein distance covered in one evolution step",
        "array": false,
        "glob": [
          ""
        ],
        "default": "3",
        "min": "1",
        "max": "50",
        "example": "3",
        "grouping": "evolution parameters",
        "position": "202",
        "required": true
      },
      "init_permissibility_vec": {
        "type": "string",
        "description": "Definition of permissible changes: (B) keep fixed, (+) delete or flip, (x) flip only",
        "array": false,
        "glob": [
          ""
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "B2:10/+11:16/B27:50/x52:55/B57/+62:66",
        "grouping": "evolution parameters",
        "position": "203",
        "required": true
      },
      "temperature": {
        "type": "number",
        "description": "Sampling temperature (currently fixed to T=100)",
        "array": false,
        "glob": [
          ""
        ],
        "default": "100",
        "min": "100",
        "max": "100",
        "example": "100",
        "grouping": "evolution parameters",
        "position": "204",
        "required": true
      },
      "scoring_metrics": {
        "type": "string",
        "description": "type of scoring metrics.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "pseudolikelihood,mean plddt,affinity",
        "min": "",
        "max": "",
        "example": "pseudolikelihood,mean plddt,affinity",
        "grouping": "evolution parameters",
        "position": "301",
        "required": true
      },
      "scoring_weights": {
        "type": "string",
        "description": "weighting metric contributions.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "0.7,0.2,0.1",
        "min": "",
        "max": "",
        "example": "0.7,0.2,0.1",
        "grouping": "evolution parameters",
        "position": "301",
        "required": false
      },
      "generator_flag": {
        "type": "string",
        "description": "type of generator used.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "RFdiffusion+ProteinMPNN",
        "min": "",
        "max": "",
        "example": "RFdiffusion+ProteinMPNN",
        "grouping": "evolution parameters",
        "position": "301",
        "required": false
      },
      "alphabet": {
        "type": "string",
        "description": "Alphabet used to form sequence.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "LAGVSERTIDPKQNFYMHWC",
        "min": "LAGVSERTIDPKQNFYMHWC",
        "max": "LAGVSERTIDPKQNFYMHWC",
        "example": "LAGVSERTIDPKQNFYMHWC",
        "grouping": "additional",
        "position": "301",
        "required": false
      },
    "outputs": {
      "all_outputs": {
        "type": "Array",
        "item": "File",
        "glob": [
          "*.zip"
        ]
      },
      "pdb_designs": {
        "type": "File",
        "item": "",
        "glob": [
          "*.pdb"
        ]
      },
      "summary": {
        "type": "File",
        "item": "",
        "glob": [
          "summary.csv"
        ]
      }
    }
  }
}