{
    "name": "AF2 v0.2",
    "description": "Create structure from sequence with AF2.",
    "author": "sokrypton",
    "github": "https://github.com/sokrypton/ColabFold",
    "paper": "https://www.nature.com/articles/s41592-022-01488-1",
    "task": "sequence folding",
    "baseCommand": [
    ],
    "arguments": [
    ],
    "dockerpull": "quay.io/labdao/af2@sha256:de50e9b416620d8193aa10704a0ec54b5bedb25342c6f9d0c0231257119faa7b",
    "gpuBool": true,
    "memoryGB": 12,
    "cpu": 3,
    "networkBool": true,
    "inputs": {
      "sequences": {
        "type": "file",
        "description": "fasta file with sequences to fold.",
        "array": false,
        "glob": [
          "*.fasta"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "input_sequence.fasta",
        "grouping": "input sequences",
        "position": "101",
        "required": true
      }
    },
    "pdb_structures": {
      "all_outputs": {
        "type": "Array",
        "item": "File",
        "glob": [
          "*.pdb"
        ]
      },
      "json_structures": {
        "type": "File",
        "item": "",
        "glob": [
          "*.json"
        ]
      }
    }
  }
