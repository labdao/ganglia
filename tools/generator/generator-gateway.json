{
  "name": "protein-binder-design v2.3 (beta)",
  "description": "Protein binder design tool.",
  "author": "Convexity Labs",
  "github": "",
  "paper": "",
  "taskCategory": "protein-binder-design",
  "task": "sequence design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "quay.io/labdao/generator@sha256:b2e69c6c0f514f381f04022a5c262f430efe3c7d564fa439e32e66d2240d3fbc",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "sequence_input": {
      "type": "string",
      "description": "Sequence input in the form: target_sequence,binder_sequence. Binder sequence may contain X characters to indicate masked residues.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "QVQLQESGGGL,QXCLIXXXELVTAXXXDTPG",
      "grouping": "Sequence input information",
      "position": "101",
      "required": true
    },
    "generator_scorers": {
      "type": "string",
      "description": "Generator and scorers used",
      "array": false,
      "glob": [
        ""
      ],
      "default": "RFdiff+ProteinMPNN/colabfold+prodigy",
      "min": "",
      "max": "",
      "example": "RFdiff+ProteinMPNN+ESM2",
      "grouping": "_Advanced input information",
      "position": "201",
      "required": false
    },
    "number_of_binders": {
      "type": "number",
      "description": "Number of binders designed.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "3",
      "min": "1",
      "max": "60",
      "example": "10",
      "grouping": "_Advanced input information",
      "position": "202",
      "required": true
    },
    "init_permissibility_vec": {
      "type": "string",
      "description": "Use slash notation with B, x, and *.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "B2:10/11:16/B27:50/x52:55/B57/62:66",
      "grouping": "_Advanced input information",
      "position": "203",
      "required": false
    },
    "scoring_metrics": {
      "type": "string",
      "description": "type of scoring metrics.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "pseudolikelihood,mean plddt,affinity",
      "grouping": "_Advanced input information",
      "position": "204",
      "required": false
    },
    "selector": {
      "type": "string",
      "description": "selector used",
      "array": false,
      "glob": [
        ""
      ],
      "default": "closed-door",
      "min": "",
      "max": "",
      "example": "Metropolis, open-door, and closed-door",
      "grouping": "_Advanced input information",
      "position": "205",
      "required": false
    },
    "temperature": {
      "type": "number",
      "description": "Sampling temperature (currently fixed to T=1)",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "1",
      "example": "1",
      "grouping": "_Advanced input information",
      "position": "206",
      "required": false
    },
    "num_designs": {
      "type": "number",
      "description": "Number of designs proposed by RFdiffusion",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "1",
      "example": "1",
      "grouping": "_RFdiffusion",
      "position": "301",
      "required": false
    },
    "num_seqs": {
      "type": "number",
      "description": "Number of sequences from protein mpnn",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "2",
      "max": "5",
      "example": "1",
      "grouping": "_Protein MPNN",
      "position": "401",
      "required": false
    }
  },
  "outputs": {
    "all_outputs": {
      "type": "Array",
      "item": "File",
      "glob": [
        "*.zip"
      ]
    },
    "pdb_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.pdb"
      ]
    },
    "summary": {
      "type": "File",
      "item": "",
      "glob": [
        "summary.csv"
      ]
    }    
  }
}