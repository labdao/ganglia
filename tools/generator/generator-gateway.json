{
  "name": "protein-binder-design v1.6 (beta)",
  "description": "Protein binder design tool.",
  "author": "Convexity Labs",
  "github": "",
  "paper": "",
  "taskCategory": "protein-binder-design",
  "task": "sequence design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "quay.io/labdao/generator@sha256:83c384a6c429e79ede30cc15c181fe35d67decb95b88ee082044914f1bc4dfe6",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "sequence_input": {
      "type": "string",
      "description": "Sequence input in the form: target_sequence,binder_sequence. Binder sequence may contain X characters to indicate masked residues.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "QVQLQESGGGL,QXCLIXXXELVTAXXXDTPG",
      "grouping": "Sequence input information",
      "position": "101",
      "required": true
    },
    "binder_chain": {
      "type": "string",
      "description": "binder chain label",
      "array": false,
      "glob": [
        ""
      ],
      "default": "B",
      "min": "",
      "max": "",
      "example": "B",
      "grouping": "_Advanced input information",
      "position": "201",
      "required": false
    },
    "target_chain": {
      "type": "string",
      "description": "target chain label",
      "array": false,
      "glob": [
        ""
      ],
      "default": "A",
      "min": "",
      "max": "",
      "example": "A",
      "grouping": "_Advanced input information",
      "position": "202",
      "required": false
    },
    "init_permissibility_vec": {
      "type": "string",
      "description": "Use slash notation with B, x, and *.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "B2:10/11:16/B27:50/x52:55/B57/62:66",
      "grouping": "_Advanced input information",
      "position": "203",
      "required": false
    },
    "generators": {
      "type": "string",
      "description": "type of generator used.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "RFdiffusion+ProteinMPNN",
      "min": "",
      "max": "",
      "example": "RFdiffusion+ProteinMPNN",
      "grouping": "Sampler parameters",
      "position": "301",
      "required": true
    },
    "speedup": {
      "type": "boolean",
      "array": false,
      "description": "Flag to evolve sequences.",
      "glob": [""],
      "default": false,
      "min": "",
      "max": "",
      "example": "false, true",
      "grouping": "_Sampler parameters",
      "position": "302",
      "required": true
    },
    "scoring_metrics": {
      "type": "string",
      "description": "type of scoring metrics.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "pseudolikelihood,mean plddt,affinity",
      "grouping": "_Sampler parameters",
      "position": "303",
      "required": false
    },
    "number_of_evo_cycles": {
      "type": "number",
      "description": "Number of evolutionary cycles",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "1",
      "max": "60",
      "example": "10",
      "grouping": "_Sampler parameters",
      "position": "304",
      "required": true
    },
    "max_levenshtein_step_size": {
      "type": "number",
      "description": "Maximum Levenshtein distance covered in one evolution step",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "50",
      "example": "3",
      "grouping": "_Sampler parameters",
      "position": "305",
      "required": false
    },
    "temperature": {
      "type": "number",
      "description": "Sampling temperature (currently fixed to T=1)",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "1",
      "example": "1",
      "grouping": "_Sampler parameters",
      "position": "306",
      "required": true
    },
    "num_designs": {
      "type": "number",
      "description": "Number of designs proposed by RFdiffusion",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "1",
      "max": "32",
      "example": "2",
      "grouping": "_RFdiffusion",
      "position": "401",
      "required": false
    },
    "num_seqs": {
      "type": "number",
      "description": "Number of sequences from protein mpnn",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "2",
      "max": "5",
      "example": "1",
      "grouping": "_Protein MPNN",
      "position": "501",
      "required": false
    }
  },
  "outputs": {
    "all_outputs": {
      "type": "Array",
      "item": "File",
      "glob": [
        "*.zip"
      ]
    },
    "pdb_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.pdb"
      ]
    },
    "summary": {
      "type": "File",
      "item": "",
      "glob": [
        "summary.csv"
      ]
    }    
  }
}