{
  "name": "protein-binder-design v2.8 (beta)",
  "description": "Define your protein-binder-design problem by entering sequence information. First, type out the sequence of the target, then put a comma, and then type out whatever information you might have about the binder sequence. The binder information can be incomplete. Whenever you have no information about a resiude, just write an X, and the binder-design method is will design it for you.",
  "author": "Convexity Labs",
  "github": "",
  "paper": "",
  "taskCategory": "protein-binder-design",
  "task": "sequence design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "quay.io/labdao/generator@sha256:ce3c15c147771711f39c90264b81b9c550e2824ec9c715e5d1b0a18acbf6748d",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "sequence_input": {
      "type": "string",
      "description": "Sequence input in the form: target_sequence,binder_sequence. Binder sequence may contain X characters to indicate masked residues.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "QVQLQESGGGL,QXCLIXXXELVTAXXXDTPG",
      "grouping": "Sequence input information",
      "position": "101",
      "required": true
    },
    "generator_scorers": {
      "type": "string",
      "description": "Generator and scorers used",
      "array": false,
      "glob": [
        ""
      ],
      "default": "RFdiff+ProteinMPNN/colabfold+prodigy",
      "min": "",
      "max": "",
      "example": "RFdiff+ProteinMPNN+ESM2",
      "grouping": "_Advanced input information",
      "position": "201",
      "required": false
    },
    "init_permissibility_vec": {
      "type": "string",
      "description": "Use slash notation with B and x.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "B2:10/11:16/B27:50/x52:55/B57/62:66",
      "grouping": "_Advanced input information",
      "position": "202",
      "required": false
    }
  },
  "outputs": {
    "pdb_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.pdb"
      ]
    },
    "trb_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.trb"
      ]
    },
    "fasta_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.fa"
      ]
    },
    "summary": {
      "type": "File",
      "item": "",
      "glob": [
        "summary.csv"
      ]
    }    
  }
}