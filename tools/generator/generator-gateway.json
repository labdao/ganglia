{
  "name": "generator v0.91 (beta)",
  "description": "Squence sampling tool.",
  "author": "Convexity Labs",
  "github": "...",
  "paper": "...",
  "taskCategory": "protein-binder-design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "pf/generator-deployment-test:latest",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "binder_template_sequence": {
      "type": "file",
      "description": "Binder template from which the sequence sampling is initialised",
      "array": false,
      "glob": [
        ".fasta"
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "binder_template_sequence.fasta",
      "grouping": "sequence input information",
      "position": "101",
      "required": true
    },
    "binder_chain": {
      "type": "string",
      "description": "binder chain label",
      "array": false,
      "glob": [
        ""
      ],
      "default": "B",
      "min": "",
      "max": "",
      "example": "B",
      "grouping": "sequence input information",
      "position": "102",
      "required": true
    },
    "target_seq": {
      "type": "string",
      "description": "Sequence of target protein.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "QVQLQESGGGL",
      "grouping": "sequence input information",
      "position": "103",
      "required": true
    },
    "target_chain": {
      "type": "string",
      "description": "target chain label",
      "array": false,
      "glob": [
        ""
      ],
      "default": "A",
      "min": "",
      "max": "",
      "example": "A",
      "grouping": "sequence input information",
      "position": "104",
      "required": true
    },
    "init_permissibility_vec": {
      "type": "string",
      "description": "Use slash notation with B, x, and *.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "B2:10/11:16/B27:50/x52:55/B57/62:66",
      "grouping": "sequence input information",
      "position": "105",
      "required": false
    },
    "generators": {
      "type": "string",
      "description": "type of generator used.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "RFdiffusion+ProteinMPNN",
      "min": "",
      "max": "",
      "example": "RFdiffusion+ProteinMPNN",
      "grouping": "Sampler parameters",
      "position": "201",
      "required": false
    },
    "scorers": {
      "type": "string",
      "description": "type of generator used.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "Colabfold",
      "min": "",
      "max": "",
      "example": "Colabfold,prodigy",
      "grouping": "Sampler parameters",
      "position": "202",
      "required": false
    },
    "scoring_metrics": {
      "type": "string",
      "description": "type of scoring metrics.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "pseudolikelihood,mean plddt,affinity",
      "grouping": "Sampler parameters",
      "position": "203",
      "required": false
    },
    "evolve": {
      "type": "boolean",
      "array": false,
      "description": "Evolution of sequences across iterations.",
      "glob": [""],
      "default": true,
      "min": "",
      "max": "",
      "example": "",
      "grouping": "Sampler parameters",
      "position": "204",
      "required": true
      },
    "number_of_evo_cycles": {
      "type": "number",
      "description": "Number of evolutionary cycles",
      "array": false,
      "glob": [
        ""
      ],
      "default": "15",
      "min": "1",
      "max": "60",
      "example": "10",
      "grouping": "Sampler parameters",
      "position": "205",
      "required": true
    },
    "max_levenshtein_step_size": {
      "type": "number",
      "description": "Maximum Levenshtein distance covered in one evolution step",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "50",
      "example": "3",
      "grouping": "Sampler parameters",
      "position": "206",
      "required": false
    },
    "temperature": {
      "type": "number",
      "description": "Sampling temperature (currently fixed to T=1)",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "1",
      "example": "1",
      "grouping": "Sampler parameters",
      "position": "207",
      "required": true
    },
    "num_designs": {
      "type": "number",
      "description": "Number of designs proposed by RFdiffusion",
      "array": false,
      "glob": [
        ""
      ],
      "default": "2",
      "min": "1",
      "max": "32",
      "example": "2",
      "grouping": "RFdiffusion",
      "position": "301",
      "required": false
    },
    "num_seqs": {
      "type": "number",
      "description": "Number of sequences from protein mpnn",
      "array": false,
      "glob": [
        ""
      ],
      "default": "1",
      "min": "1",
      "max": "5",
      "example": "1",
      "grouping": "Protein MPNN",
      "position": "401",
      "required": false
    },
    "alphabet": {
      "type": "string",
      "description": "Alphabet used to form sequence. Currently fixed.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "LAGVSERTIDPKQNFYMHWC",
      "min": "LAGVSERTIDPKQNFYMHWC",
      "max": "LAGVSERTIDPKQNFYMHWC",
      "example": "LAGVSERTIDPKQNFYMHWC",
      "grouping": "additional",
      "position": "501",
      "required": false
    }
  },
  "outputs": {
    "all_outputs": {
      "type": "Array",
      "item": "File",
      "glob": [
        "*.zip"
      ]
    },
    "pdb_designs": {
      "type": "File",
      "item": "",
      "glob": [
        "*.pdb"
      ]
    },
    "summary": {
      "type": "File",
      "item": "",
      "glob": [
        "summary.csv"
      ]
    }    
  }
}