{
    "class": "CommandLineTool",
    "name": "dlkcat",
    "description": "batch predict enzyme catalytic activity from a protein sequence and molecule smile",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
        "conda run -n env python prediction_for_input.py /inputs/input.tsv && mv output.tsv /outputs/"
    ],
    "dockerPull": "docker.io/mcmenemy/plex-dlkcat@sha256:9442c583098d344cfe3281cf3ccc657d62e5182b33fe3affe82c772ea0175397",
    "gpuBool": false,
    "networkBool": false,
    "inputs": {
        "input_tsv": {
            "type": "File",
            "glob": ["input.tsv"]
        }
    },
    "outputs": {
        "scores": {
            "type": "File",
            "glob": ["output.tsv"]
        }
    }
}

