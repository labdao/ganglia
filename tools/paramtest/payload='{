        payload='{
          "name": "test all params ",
          "walletAddress": "0xab5801a7d398351b8be11c439e05c5b3259aec9b",
          "toolCid": "QmdnRYRyZytjrcC8Q9y7g5zMe1nSQyUJmxvZX1SMKB8tQT",
          "scatteringMethod": "crossProduct",
          "kwargs": {
            "first_file": ["QmfQqo1kUYNuyCJRRYj7LPRjPdoiiH1E9KjY8wzURBKKxE/example_first_file.txt"],
            "file_array": [["QmSuMCJ24jj3E4Uu3QdVT4iKDu1KPgJ9unnQBe7yJ9QEo7/array_0_file.txt", "QmWbF24RHTZyocEhCAwDpjWoxy47PgG9hGNGYYN1jnpiW7/array_1_file.txt", "QmbmHzQM7gX5gVvFr6nHqnUjGzprLEeuahLCSjBDDJs2WF/array_2_file.txt"]],
            "first_string": ["example first string"],
            "string_array": [["string 1", "string 2", "string 3"]],
            "first_number": [3],
            "number_array": [[2,4,8]],
            "first_bool": [true],
            "bool_array": [[false, true, false]]
          }
        }'


        curl -X POST "http://localhost:8080/flows" \
               -H "Content-Type: application/json" \
               -d "$payload"



docker build -t paramtest .
