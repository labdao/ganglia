{
    "class": "CommandLineTool",
    "name": "colabfold-mini gateway",
    "description": "Protein folding prediction using Colabfold (mini settings)",
    "author": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
      "colabfold_batch --templates --max-msa 32:64 --num-recycle 1 /inputs/sequence /outputs;"
    ],
    "dockerPull": "public.ecr.aws/p7l9w5o7/colabfold:latest",
    "gpuBool": true,
    "networkBool": true,
    "memoryGB": 12,
    "cpu": 1.5,
    "inputs": {
      "sequence": {
        "type": "File",
        "item": "",
        "glob": ["*.fasta"]
      }
    },
    "outputs": {
      "best_folded_protein": {
        "type": "File",
        "item": "",
        "glob": ["*rank_1*.pdb"]
      },
      "all_folded_proteins": {
        "type": "Array",
        "item": "File",
        "glob": ["*rank*.pdb"]
      }
    }
}
