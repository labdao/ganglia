{
    "name": "ColabFold v2.7",
    "description": "Create structure from sequence with ColabFold.",
    "author": "sokrypton",
    "github": "https://github.com/sokrypton/ColabFold",
    "paper": "https://www.nature.com/articles/s41592-022-01488-1",
    "task": "protein folding",
    "baseCommand": [
    ],
    "arguments": [
    ],
    "dockerPull": "quay.io/labdao/af2@sha256:4a528fc9b5a9f50c625ca3586d3b494c2f556de72770a6b1aa1a06ed3e94ad90",
    "gpuBool": true,
    "memoryGB": 12,
    "cpu": 3,
    "networkBool": true,
    "inputs": {
      "sequences": {
        "type": "file",
        "description": "fasta file with sequences to fold.",
        "array": false,
        "glob": [
          "*.fasta"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "input_sequence.fasta",
        "grouping": "input sequences",
        "position": "101",
        "required": false
      },
      "pdb": {
        "type": "file",
        "description": "pdb file with sequence to fold.",
        "array": false,
        "glob": [
          "*.pdb"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "input_pdb.pdb",
        "grouping": "input sequences",
        "position": "102",
        "required": false
      }
    },
    "pdb_structures": {
      "all_outputs": {
        "type": "Array",
        "item": "File",
        "glob": [
          "*.pdb"
        ]
      },
      "json_structures": {
        "type": "File",
        "item": "",
        "glob": [
          "*.json"
        ]
      },
      "csv_summary": {
        "type": "File",
        "item": "",
        "glob": [
          "*.csv"
        ]
      }
    }
  }
