{
  "class": "CommandLineTool",
  "name": "equibind gateway v0.3",
  "description": "Docking of small molecules to a protein",
  "author": "HannesStark",
  "baseCommand": [],
  "arguments": [],
  "dockerPull": "ghcr.io/labdao/equibind:main@sha256:21a381d9ab1ff047565685044569c8536a55e489c9531326498b28d6b3cc244f",
  "gpuBool": false,
  "networkBool": false,
  "memoryGB": 5,
  "cpu": 0.8,
  "inputs": {
    "protein": {
      "type": "File",
      "description": "Protein structure file",
      "array": false,
      "glob": ["*.pdb"],
      "default": "example_protein.pdb",
      "min": "",
      "max": "",
      "example": "example_protein.pdb",
      "grouping": "",
      "position": "1",
      "required": true
    },
    "small_molecule": {
      "type": "File",
      "description": "Small molecule file",
      "array": false,
      "glob": ["*.sdf", "*.mol2"],
      "default": "example_molecule.sdf",
      "min": "",
      "max": "",
      "example": "example_molecule.sdf",
      "grouping": "",
      "position": "2",
      "required": true
    }
  },
  "outputs": {
    "best_docked_small_molecule": {
      "type": "File",
      "item": "",
      "glob": ["*_docked.sdf", "*_docked.mol2"]
    },
    "protein": {
      "type": "File", 
      "item": "",
      "glob": ["*.pdb"]
    },
    "message": {
      "type": "File",
      "item": "",
      "glob": ["*.txt"]
    }
  }
}
