{
    "class": "CommandLineTool",
    "name": "nonfileparamtest",
    "description": "test nonfile params",
    "author": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
        "python -u main.py $(inputs.mystring.value) $(inputs.myint.value)"
    ],
    "dockerPull": "docker.io/openzyme/nonfileparamtest@sha256:a1d0082f886a5824f6e50acd2a6e0017e4a953743bae64bb5c9ff575cc17b0e8",
    "gpuBool": false,
    "networkBool": false,
    "inputs": {
        "fileparam": {
            "type": "File",
            "glob": ["*.txt"]
        },
        "mystring": {
            "type": "string",
            "default": "exampleString"
        },
        "myint": {
            "type": "int",
            "default": "123"
        }
    },
    "outputs": {
        "result": {
            "type": "File",
            "glob": ["result.txt"]
        }
    }
}
