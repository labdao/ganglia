{
  "name": "Protein MPNN v2.2",
  "description": "Protein MPNN.",
  "author": "Dauparas, J. et al.",
  "github": "https://github.com/dauparas/ProteinMPNN/tree/main",
  "paper": "https://www.science.org/doi/10.1126/science.add2187",
  "task": "protein design",
  "baseCommand": [
  ],
  "arguments": [
  ],
