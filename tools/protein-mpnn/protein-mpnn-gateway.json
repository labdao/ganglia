  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "protein_complex": {
      "type": "file",
      "description": "Protein complex.",
      "array": false,
      "glob": [
        "*.pdb"
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "my_target_protein.pdb",
      "grouping": "protein complex",
      "position": "101",
      "required": true
    },
    "chains_to_design": {
      "type": "string",
      "description": "The chain of the target protein to design against.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "A",
      "min": "",
      "max": "",
      "example": "A B",
      "grouping": "protein complex",
      "position": "102",
      "required": true
    },
    "num_seqs": {
      "type": "number",
      "description": "Number of design sequences.",
      "array": false,
      "glob": [
        ""
      ],
      "default": 8,
      "min": "",
      "max": "",
      "example": 8,
