{
  "name": "Protein MPNN v1.0",
  "description": "Protein MPNN.",
  "author": "Dauparas, J. et al.",
  "github": "https://github.com/dauparas/ProteinMPNN/tree/main",
  "paper": "https://www.science.org/doi/10.1126/science.add2187",
  "task": "protein design",
  "baseCommand": [
  ],
  "arguments": [
  ],
  "dockerPull": "quay.io/...",
  "gpuBool": true,
  "memoryGB": 12,
  "cpu": 3,
  "networkBool": true,
  "inputs": {
    "protein_complex": {
      "type": "file",
      "description": "Protein complex.",
      "array": false,
      "glob": [
        "*.pdb"
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "my_target_protein.pdb",
      "grouping": "protein complex",
      "position": "101",
      "required": true
    },
    "chains_to_design": {
      "type": "string",
      "description": "The chain of the target protein to design against.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "A",
      "min": "",
      "max": "",
      "example": "A B",
      "grouping": "protein complex",
      "position": "102",
      "required": true
    },
    "num_seq": {
      "type": "number",
      "description": "Number of design sequences.",
      "array": false,
      "glob": [
        ""
      ],
      "default": "8",
      "min": "",
      "max": "",
      "example": "8",
      "grouping": "protein complex",
      "position": "103",
      "required": true
    },
    "mpnn_sampling_temp": {
      "type": "number",
      "description": "mpnn smapling temperature",
      "array": false,
      "glob": [
        ""
      ],
      "default": "",
      "min": "",
      "max": "",
      "example": "0.1",
      "grouping": "protein complex",
      "position": "104",
      "required": true
    },
    "use_solubleMPNN": {
      "type": "bool",
      "description": "use soluble",
      "array": false,
      "glob": [
        ""
      ],
      "default": true,
      "min": "",
      "max": "",
      "example": "true",
      "grouping": "protein complex",
      "position": "105",
      "required": true
    },
    "initial_guess": {
      "type": "bool",
      "default": "true",
      "min": "",
      "max": "",
      "description": "use initial guess",
      "grouping": "binder",
      "position": "106",
      "required":true
  },
  "rm_aa": {
      "type": "int",
      "default": "C",
      "min": "1",
      "max": "64",
      "description": "The number of protein binders to be designed",
      "grouping": "binder",
      "position": "107",
      "required": true
  }
  },
  "outputs": {
    "output_fasta": {
      "type": "File",
      "item": "",
      "glob": [
        "*.fa"
      ]
    }
  }
}
