      "position": "201",
      "required": true
    },
    "mpnn_sampling_temp": {
      "type": "string",
      "description": "mpnn sampling temperature",
      "array": false,
      "glob": [
        ""
      ],
      "default": "0.1",
      "min": "",
      "max": "",
      "example": "0.1",
      "grouping": "Design parameters",
      "position": "202",
      "required": true
    },
    "use_solubleMPNN": {
      "type": "boolean",
      "description": "",
      "array": false,
      "glob": [
        ""
      ],
      "default": true,
      "min": "",
      "max": "",
      "example": true,
      "grouping": "Design parameters",
      "position": "203",
      "required": true
    },
    "initial_guess": {
      "type": "boolean",
      "description": "",
      "array": false,
      "glob": [
        ""
      ],
      "default": true,
      "min": "",
      "max": "",
      "example": true,
      "grouping": "Design parameters",
      "position": "204",
      "required": true
    },
    "rm_aa": {
      "type": "string",
      "description": "",
      "array": false,
      "glob": [
        ""
      ],
      "default": "C",
      "min": "",
      "max": "",
      "example": "C",
      "grouping": "Design parameters",
      "position": "205",
      "required": true
    }
  },
  "outputs": {
    "output_fasta": {
      "type": "File",
      "item": "",
      "glob": [
        "*.fa"
      ]
