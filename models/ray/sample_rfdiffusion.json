{
    "name": "relay_sample_rfdiffusion_both_ipfs_s3_v5",
    "description": "Sample RFDiffusion model",
    "task": "protein design",
    "taskCategory": "protein-binder-design",
    "modelType": "ray",
    "rayServiceEndpoint": "/sample_relay_rf_diffusion",
    "checkpointCompatible": true,
    "xAxis": "plddt",
    "yAxis": "i_pae",
    "metricsDescription": "*Stability Score:* larger value indicates higher confidence in the predicted local structure\n*Affinity Score:* larger value indicates higher confidence in the predicted interface structure\n*Note:* designs which lie within the green square are recommended for laboratory testing",
    "computeCost": 10,
    "inputs": {
      "pdb": {
        "type": "file",
        "description": "The target protein to design against.",
        "array": false,
        "glob": [
          "*.pdb"
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "my_target_protein.pdb",
        "grouping": "target",
        "position": "101",
        "required": true
      },
      "target_chain": {
        "type": "string",
        "description": "The chain of the target protein to design against.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "A",
        "min": "",
        "max": "",
        "example": "A",
        "grouping": "target",
        "position": "102",
        "required": true
      },
      "binder_chain": {
        "type": "string",
        "description": "The chain of the target protein to design against.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "B",
        "min": "",
        "max": "",
        "example": "B",
        "grouping": "_advanced",
        "position": "201",
        "required": false
      },
      "binder_length": {
        "type": "number",
        "description": "The length of the protein binder that is being designed",
        "array": false,
        "glob": [
          ""
        ],
        "default": "85",
        "min": "1",
        "max": "1000",
        "example": "80",
        "grouping": "_advanced",
        "position": "202",
        "required": false
      },
      "contig": {
        "type": "string",
        "default": "",
        "description": "The complete prompt or 'contig' passed to the diffusion model. This overrides other GUI inputs.",
        "example": "A10:110/B4:5/x6:8/B9:50/x51:71",
        "grouping": "_advanced",
        "position": "203",
        "required": false
      },
      "hotspots": {
        "type": "string",
        "description": "The residues that should be in contact with the binder. e.g. A30,A33,A34",
        "array": false,
        "glob": [
          ""
        ],
        "default": "",
        "min": "",
        "max": "",
        "example": "A30,A33,A34",
        "grouping": "_advanced",
        "position": "204",
        "required": false
      },
      "start_residue": {
        "type": "number",
        "description": "The start residue of the target protein.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "5",
        "min": "1",
        "max": "",
        "example": "1",
        "grouping": "_advanced",
        "position": "205",
        "required": false
      },
      "end_residue": {
        "type": "number",
        "description": "The end residue of the target protein.",
        "array": false,
        "glob": [
          ""
        ],
        "default": "107",
        "min": "1",
        "max": "",
        "example": "120",
        "grouping": "_advanced",
        "position": "206",
        "required": false
      }
    },
    "outputs": {
      "string_message": {
        "type": "File",
        "glob": ["*.json"]
      }
    }
}